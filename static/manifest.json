{
"orientation": "any", 
  "name": "Diagnoz",
  "short_name": "Diagnoz",
  "description": "Yassin Progressive Web Application",
  "start_url": "/",
  
  "display": "standalone",
  "background_color": "#194D33",
  "theme_color": "#000000",
  "id": "yasso12",
  "dir": "ltr",
  "lang": "en",
  "edge_side_panel.preferred_width":"12px",
  "iarc_rating_id": "5",
  "display_override": [
    "fullscreen",
    "window-controls-overlay"
  ],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+jngl",
      "url": "/lookup?type=%s"
    },
    {
      "protocol": "web+jnglstore",
      "url": "/shop?for=%s"
    }
  ],
  
  
  "shortcuts": [
    {
      "name":"about",
      "url":"/about",
      "description":"opens the about page",
      "icons": [
        {
          "src": "/static/images/manifest-icon-192.maskable.png",
          "type": "image/png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  

  "icons": [
    {
      "src": "/static/images/512Logo.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/static/images/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/static/images/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/static/images/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "Flask-Disease/static/images/screenshots/1280x800-screenshot.png",
      "sizes": "1280x800",
      "type": "image/png"
    },
    {
      "src": "Flask-Disease/static/images/screenshots/750x1334-screenshot.png",
      "sizes": "750x1334",
      "type": "image/png"
    }
  ]
